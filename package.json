{
  "name": "exp-sql-template",
  "version": "1.0.0",
  "description": "A Node.js project for deploying with Vercel, using Express, Sequelize, and PostgreSQL.",
  "main": "index.js",
  "type": "module",
  "bin": {
    "exp-sql-template": "./bin/cli.js"
  },
  "scripts": {
    "start": "node ./src/server.js",
    "dev": "node --env-file .env --watch ./src/server.js",
    "lint": "eslint --fix . --ext .js",
    "format": "prettier --write ."
  },
  "keywords": [
    "vercel",
    "express",
    "sequelize",
    "postgresql",
    "api"
  ],
  "author": "Gabriel Martinez",
  "license": "ISC",
  "dependencies": {
    "cors": "2.8.5",
    "express": "4.19.2",
    "helmet": "7.1.0",
    "pg": "8.12.0",
    "pg-hstore": "2.3.4",
    "sequelize": "6.37.3"
  },
  "devDependencies": {
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "morgan": "1.10.0",
    "prettier": "3.3.3",
    "standard": "17.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "eslintConfig": {
    "extends": [
      "./node_modules/standard/eslintrc.json",
      "plugin:prettier/recommended"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrielmartinezghioma/template-exp-sq-module.git"
  },
  "homepage": "https://github.com/gabrielmartinezghioma/template-exp-sq-module/blob/main/readme.md"
}
