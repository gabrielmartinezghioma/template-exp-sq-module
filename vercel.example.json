{
  "version": 2,
  "buildCommand": "npm start",
  "devCommand": "npm run dev",
  "framework": "node",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "src/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/server.js"
    }
  ]
}